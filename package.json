{
  "name": "angular-add-test-to-existing-component",
  "displayName": "Angular Add Test To Existing Component",
  "description": "Adds a unit test to an angular component generated with --skip-tests",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ng-generate-spec.create",
        "title": "Add Test to Existing Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ng-generate-spec.create",
          "when": "resourceFilename =~ /\\.component\\.ts$/",
          "group": "z_commands"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  },
  "publisher": "DanWeisz",
  "author": {
    "name": "Dan Weisz",
    "email": "weisz.danny+vscode@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danredred/angular-add-test-to-existing-component.git"
  },
  "icon": "Avocato.webp"
}
